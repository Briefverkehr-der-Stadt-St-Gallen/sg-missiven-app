            <param name="value" value="(&lt;a href=&#34;https://www.ssrq-sds-fds.ch/lemma-db-edit/views/view-keyword.xq?id={@xml:id}&#34; target=&#34;_blank&#34;&gt;{desc/string()}&lt;/a&gt;, if(gloss) then(&lt;span&gt; ({gloss/string()})&lt;/span&gt;) else ())"/>
            <pb:template xmlns="" xml:space="preserve"><li data-ref="[[ref]]">
  <div>[[value]]</div>
</li></pb:template>
        </model>
                </elementSpec>
            <elementSpec ident="docImprint" mode="change">
                    <model behaviour="listItem">
            <param name="content" value="(ec:label('imprint'), ec:colon(), translate((translate((pubPlace/text() || &#34; &#34; || publisher/text() ),&#34;)&#34;,&#34;&#34;)),&#34;(&#34;,&#34;&#34;))"/>
        </model>
                </elementSpec>
    <elementSpec ident="graphic" mode="change">
                    <model behaviour="graphic">
            <param name="url" value="'resources/images/' || @url"/>
            <param name="width" value="@width"/>
            <param name="height" value="@height"/>
            <param name="scale" value="@scale"/>
            <param name="title" value="desc"/>
        </model>
                </elementSpec>
            <elementSpec ident="profileDesc" mode="change">
                    <modelSequence predicate="$parameters?header='short'">
            <model behaviour="block">
                <desc>Datum</desc>
                <param name="content" value="ec:format-date-correspAction(.//correspAction[@type='sent']/date)"/>
            </model>
            <model behaviour="inline">
                <desc>Absender</desc>
                <param name="content" value="let $ids:= .//correspAction[@type='sent']/(persName/@ref, orgName/@ref) return string-join(ec:get-name-list($ids), '; ')"/>
                <pb:template xmlns="" xml:space="preserve">Absender: [[content]]</pb:template>
            </model>
            <model behaviour="block">
                <desc>Empfänger</desc>
                <param name="content" value="let $ids:= .//correspAction[@type='received']/(persName/@ref, orgName/@ref) return string-join(ec:get-name-list($ids), '; ')"/>
                <pb:template xmlns="" xml:space="preserve">Empfänger: [[content]]</pb:template>
            </model>
        </modelSequence>
                    <model behaviour="omit"/>
                </elementSpec>
            <elementSpec ident="item" mode="change">
                    <model behaviour="listItem">
            <param name="label" value="label[@type=&#34;main&#34;]/text()"/>
            <param name="id" value="@xml:id"/>
            <pb:template xmlns="" xml:space="preserve"><li data-ref="[[id]]">
  <paper-checkbox class="select-facet" title="i18n(highlight-facet)"/>
  <div>
    <a href="../begriffe/[[label]]?key=[[id]]">[[label]]</a>
  </div>
</li></pb:template>
        </model>
                </elementSpec>
            <elementSpec ident="personGrp" mode="add">
                    <model predicate="$parameters?header='context'" behaviour="pass-through">
            <param name="content" value="."/>
            <param name="value" value="&lt;a href=&#34;../namen/{./persName[@type='main']/string()}?key={@xml:id}&#34;&gt;{persName[@type='full']/string()}&lt;/a&gt;"/>
            <param name="ref" value="@xml:id"/>
            <pb:template xmlns="" xml:space="preserve"><li data-ref="[[ref]]">
  <paper-checkbox class="select-facet" title="i18n(highlight-facet)"/>
  <div>[[value]]</div>
</li></pb:template>
        </model>
                    <modelSequence>
            <model behaviour="inline">
                <param name="content" value="(ec:label('organisation'), ' ')"/>
            </model>
            <model behaviour="link">
                <param name="link" value="'../namen/' || persName[@type eq 'main']/string() || '?key=' || @xml:id"/>
                <param name="content" value="persName[@type eq 'full']/string()"/>
                <param name="target" value="'_new'"/>
            </model>
            <model behaviour="inline">
                <param name="content" value="' ' || @type/string()"/>
            </model>
        </modelSequence>
                </elementSpec>
            <elementSpec ident="ex" mode="change">
                    <model predicate="@cert eq 'medium'" behaviour="pass-through">
            <param name="content" value="('[', ., ']')"/>
            <param name="alternate" value="'Unsichere Auflösung von Abkürzung'"/>
            <pb:template xmlns="" xml:space="preserve"><pb-popover>
      <span slot="default" class="text-critical">[[content]]</span>
      <template slot="alternate">[[alternate]]</template>
</pb-popover></pb:template>
        </model>
                    <model behaviour="pass-through">
            <param name="content" value="('[', ., ']')"/>
            <param name="alternate" value="'Auflösung von Abkürzung'"/>
            <pb:template xmlns="" xml:space="preserve"><pb-popover>
      <span slot="default" class="text-critical">[[content]]</span>
      <template slot="alternate">[[alternate]]</template>
</pb-popover></pb:template>
        </model>
                </elementSpec>
            </schemaSpec>
        </body>
    </text>
</TEI>